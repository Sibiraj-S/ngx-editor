{
  "name": "ngx-editor",
  "version": "4.1.0",
  "description": "WYSIWYG Editor for Angular Applications",
  "license": "MIT",
  "scripts": {
    "build": "ng build --prod",
    "build:package": "sh ./scripts/build.sh",
    "build:docs": "compodoc",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "lint:scripts": "eslint ./scripts",
    "ng": "ng",
    "prebuild": "npm run lint",
    "postbuild": "npm run build:package",
    "prepublishOnly": "node ./scripts/prepublish.js",
    "prerelease": "npm run build",
    "publish:docs": "npm run build:docs && node ./scripts/publishdocs.js",
    "release": "sh ./scripts/release.sh",
    "start": "ng serve --aot --open --port 5002 --host 0.0.0.0",
    "serve:docs": "compodoc --serve --open --output=\"docs/\"",
    "test": "ng test --browsers ChromeHeadless --watch false",
    "test:watch": "ng test"
  },
  "keywords": [
    "angular-editor",
    "angular-wysiwyg-editor",
    "wysiwyg-editor",
    "ngx-editor",
    "angular2-wysiwyg-editor",
    "angular4-wysiwyg-editor"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sibiraj-s/ngx-editor.git"
  },
  "bugs": {
    "url": "https://github.com/sibiraj-s/ngx-editor/issues"
  },
  "homepage": "https://github.com/sibiraj-s/ngx-editor",
  "peerDependencies": {
    "@angular/common": "~9.0.5",
    "@angular/core": "~9.0.5",
    "@angular/forms": "~9.0.5",
    "font-awesome": "^4.7.0",
    "ngx-bootstrap": "^5.5.0",
    "rxjs": "~6.5.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.5",
    "@angular/animations": "~9.0.5",
    "@angular/cli": "~9.0.5",
    "@angular/common": "~9.0.5",
    "@angular/compiler": "~9.0.5",
    "@angular/compiler-cli": "^9.0.5",
    "@angular/core": "^9.0.5",
    "@angular/forms": "~9.0.5",
    "@angular/language-service": "~9.0.5",
    "@angular/platform-browser": "~9.0.5",
    "@angular/platform-browser-dynamic": "~9.0.5",
    "@angular/router": "~9.0.5",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "bootstrap": "^4.4.1",
    "chalk": "^3.0.0",
    "codelyzer": "~5.1.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "font-awesome": "^4.7.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.3",
    "inquirer": "^7.0.6",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.2",
    "ngx-bootstrap": "^5.5.0",
    "protractor": "~5.4.3",
    "rxjs": "~6.5.4",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5",
    "zone.js": "~0.10.2"
  }
}
